<p>Students</p>
<% @students.each do |student| %>
<p>First Name</p>
<%= student.first_name%>
<p>Last Name</p>
<%= student.last_name%>
<p>Age</p>
<%= student.age%>

<a href="/students/<%= student.id %>/edit">Edit Students</a>

<form method="post" action="/students/<%= student.id %>delete">
  <input type="submit" value="delete"/></form>
  <hr/>
  <% end %>
